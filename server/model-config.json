{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongodb"
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": false
  },
  "course": {
    "dataSource": "mongodb",
    "public": true
  },
  "semester": {
    "dataSource": "mongodb",
    "public": true
  },
  "subject": {
    "dataSource": "mongodb",
    "public": true
  },
  "note": {
    "dataSource": "mongodb",
    "public": true
  },
  "container": {
    "dataSource": "uploads",
    "public": true
  }
}
